<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>jQuery_Ajax_Request</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Make Text Posts to Tumblr and Twitter - jQuery Ajax Request</description>
    <display-name>TextPost</display-name>
    <servlet-name>TextPost</servlet-name>
    <servlet-class>socialMedia.TextPost</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TextPost</servlet-name>
    <url-pattern>/TextPost</url-pattern>
  </servlet-mapping>
</web-app>